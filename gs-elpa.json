{
    "backend": "gs-elpa",
    "package": "gs_elpa",
    "repositories": {
        "gnu-elpa": {
            "repo_uri": "https://elpa.gnu.org/packages/",
            "masters": ["gentoo"]
        },
        "nongnu-elpa": {
            "repo_uri": "https://elpa.nongnu.org/nongnu/",
            "masters": ["gentoo", "gnu-elpa"]
        },
        "melpa": {
            "repo_uri": "https://melpa.org/packages/",
            "masters": ["gentoo", "gnu-elpa"]
        },
        "melpa-stable": {
            "repo_uri": "https://stable.melpa.org/packages/",
            "masters": ["gentoo", "gnu-elpa"]
        }
    },
    "common_config": {
        "exclude": [
            "@", "comment-dwim-2"
        ],
        "substitute": {
            "aio": "emacs-aio",
            "ansilove": "emacs-ansilove",
            "bazel": "emacs-bazel-mode",
            "citeproc": "citeproc-el",
            "crystal-mode": "emacs-crystal-mode",
            "el-fetch": "emacs-el-fetch",
            "org": "org-mode",
            "org-superstar": "org-superstar-mode",
            "proof-general": "proofgeneral",
            "websearch": "emacs-websearch",
            "iso-639": "iso_639"
        },
        "external": {
            "ansi-color": "virtual/emacs",
            "auth-source-pass": "virtual/emacs",
            "calc": "virtual/emacs",
            "cl-lib": "virtual/emacs",
            "cl-print": "virtual/emacs",
            "ede": "virtual/emacs",
            "eieio": "virtual/emacs",
            "emacs": "virtual/emacs",
            "epg": "virtual/emacs",
            "ert": "virtual/emacs",
            "gnus": "virtual/emacs",
            "json": "virtual/emacs",
            "semantic": "virtual/emacs",
            "tabulated-list": "virtual/emacs",
            "term": "virtual/emacs"
        }
    }
}
