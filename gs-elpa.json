{
    "backend": "gs-elpa",
    "package": "gs_elpa",
    "repositories": {
        "gnu-elpa": {
            "repo_uri": "https://elpa.gnu.org/packages/",
            "masters": ["gentoo"]
        },
        "nongnu-elpa": {
            "repo_uri": "https://elpa.nongnu.org/nongnu/",
            "masters": ["gentoo", "gnu-elpa"]
        },
        "melpa": {
            "repo_uri": "https://melpa.org/packages/",
            "masters": ["gentoo", "gnu-elpa"]
        },
        "melpa-stable": {
            "repo_uri": "https://stable.melpa.org/packages/",
            "masters": ["gentoo", "gnu-elpa"]
        }
    },
    "common_config": {
        "exclude": [
            "@", "comment-dwim-2"
        ],
        "external": {"emacs": "virtual/emacs", "cl-lib": "virtual/emacs", "eieio": "virtual/emacs"}
    }
}
